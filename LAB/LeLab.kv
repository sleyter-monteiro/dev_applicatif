#:import layout_exemples layout_exemples
#:import widget_exemples widget_exemples
#:import nav nav
#:import action_bar action_bar

<MenuPrincipal@BoxLayout>:
    orientation: 'vertical'
    Button:
        text: 'Layouts'
        on_press: app.manager.push('LayoutExemplesTabs')

    Button:
        text: 'Widgets'
        on_press: app.manager.push('WidgetExemples')

    Button:
        text: 'Images'
        on_press: app.manager.push('ImagesExemples')

    Button:
        text: 'Canvas'
        on_press: app.manager.push('CanvasExemplesTabs')   


<MyScreenManager>:
    Screen:
        name: 'MenuPrincipal'
        MenuPrincipal:

    Screen:
        name: 'ImagesExemples'
        BoxLayoutWithActionbar:
            title: 'Images'
            ImagesExemple:

    Screen:
        name: 'WidgetExemples'
        BoxLayoutWithActionbar:
            title: 'Widgets'
            WidgetsExemple:

    Screen:
        name: 'LayoutExemplesTabs'
        BoxLayoutWithActionbar:
            title: 'Layouts'
            LayoutExemplesTabs:

    Screen:
        name: 'CanvasExemplesTabs'
        BoxLayoutWithActionbar:
            title: 'Canvas'
            CanvasExemplesTabs:                                          

                 


